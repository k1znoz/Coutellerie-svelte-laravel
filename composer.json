{
    "name": "coutellerie/svelte-laravel",
    "type": "project",
    "require": {
        "php": "^8.3",
        "laravel/framework": "^12.0"
    },
    "scripts": {
        "post-install-cmd": [
            "cd services/coutellerie-laravel && composer install --no-dev --optimize-autoloader"
        ],
        "start": "cd services/coutellerie-laravel && php artisan serve --host=0.0.0.0 --port=${PORT:-8080}"
    }
}